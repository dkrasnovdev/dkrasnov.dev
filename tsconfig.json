{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$assets/*": ["src/assets/*"],
      "$components/*": ["src/components/*"],
      "$content/*": ["src/content/*"],
      "$data/*": ["src/data/*"],
      "$layouts/*": ["src/layouts/*"],
      "$store": ["src/lib/store.ts"],
      "$style/*": ["src/style/*"],
      "$utils/*": ["src/utils/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
